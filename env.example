# Environment Variables Template
# Copy this file to .env and fill in your values

# Network Configuration
TESTNET_RPC_URL=https://your-testnet-rpc-url
MAINNET_RPC_URL=https://your-mainnet-rpc-url
TESTNET_CHAIN_ID=11155111
MAINNET_CHAIN_ID=1

# Private Key (for deployment)
PRIVATE_KEY=your-private-key-here

# API Keys
ETHERSCAN_API_KEY=your-etherscan-api-key

# Gas Reporting
REPORT_GAS=true

# Oracle Configuration
GOLD_PRICE_API_KEY=your-gold-price-api-key
LSE_API_KEY=your-lse-api-key

# Compliance Configuration
COMPLIANCE_API_KEY=your-compliance-api-key
KYC_PROVIDER_API_KEY=your-kyc-provider-api-key

# Security
MULTISIG_WALLET_ADDRESS=your-multisig-wallet-address
EMERGENCY_PAUSE_ADDRESS=your-emergency-pause-address
